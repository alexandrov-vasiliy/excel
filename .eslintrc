{
  "parser":  "@typescript-eslint/parser",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": ["plugin:@typescript-eslint/recommended"],
  "rules": {
    "semi": "off",
    "linebreak-style": "off",
    "object-curly-spacing": "off",
    "require-jsdoc": "off"
  }
}
